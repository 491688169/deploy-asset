    "test": "mocha -t 10000 --compilers js:babel/register test/test-*",
    "cover": "rm -rf ./coverage && istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec -t 5000 --compilers js:babel/register test/test-* && cat ./coverage/lcov.info | ./cover.js",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec -t 5000 --compilers js:babel/register test/test-* && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
